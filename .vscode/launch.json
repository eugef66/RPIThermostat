	"version": "0.2.0",
	"configurations": [
		{
			"name": "Web",
			"type": "python",
			"request": "launch",
			"program": "web.py",
			"console": "integratedTerminal"
		},
		{
			"name": "Set",
			"type": "python",
			"request": "launch",
			"program": "set.py",
			"console": "integratedTerminal",
			"args": [
				"t=70",
				"m=COOL",
				"s=2021-02-12 13:00"
			]
		},
		{
			"name": "Get",
			"type": "python",
			"request": "launch",
			"program": "get.py",
			"console": "integratedTerminal"
		},
		{
			"name": "Process",
			"type": "python",
			"request": "launch",
			"program": "proc.py",
			"console": "integratedTerminal"
		},
		{
			"name": "Python: Current File",
			"type": "python",
			"request": "launch",
			"program": "${file}",
			"console": "integratedTerminal"
		}
	]
}